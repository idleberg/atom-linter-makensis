{
  "name": "linter-makensis",
  "version": "0.19.1",
  "description": "Atom linter plugin for NSIS, using makensis",
  "license": "MIT OR GPL-2.0",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "scripts": {
    "build": "npm run clean && rollup --config",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:js": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "start": "rollup --config --watch",
    "test": "npm run lint",
    "prepare": "husky install"
  },
  "keywords": [
    "nsis",
    "linter",
    "makensis"
  ],
  "main": "./lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-linter-makensis.git"
  },
  "engines": {
    "atom": ">=1.56.0 <2.0.0"
  },
  "activationHooks": [
    "language-nsis:grammar-used"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "package-deps": [
    {
      "name": "linter",
      "minimumVersion": "2.0.0"
    },
    {
      "name": "linter-ui-default"
    }
  ],
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@types/atom": "^1.40.5",
    "@types/node": "^16.7.4",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^8.0.0",
    "atom-satisfy-dependencies": "^0.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "log-symbols": "^5.0.0",
    "makensis": "^1.0.2",
    "micromatch": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "open": "^8.2.0",
    "remark-cli": "^10.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-atomic": "^3.0.1",
    "tslib": "^2.3.1",
    "typescript": ">=4.3 <4.4"
  },
  "lint-staged": {
    "*.(json|ts)": "eslint --cache --fix"
  }
}
